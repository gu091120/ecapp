{
  "name": "easy_create_app",
  "version": "1.2.0",
  "description": "",
  "main": "/bin/capp.js",
  "scripts": {
    "start": "node index.js",
    "changelog": "cp package.json _package.json &&preset=`conventional-commits-detector` && echo $preset && bump=`conventional-recommended-bump -p angular` && echo ${1:-$bump} && npm --no-git-tag-version version ${1:-$bump} &>/dev/null && conventional-changelog -i CHANGELOG.md -s -p ${2:-$preset} && git add CHANGELOG.md package.json package-lock.json && version=`cat package.json` && git commit -m'docs(CHANGELOG): $version' && mv -f _package.json package.json && npm version ${1:-$bump} -m 'chore(release): %s' && git push --follow-tags "
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --pattern '**/*.*(js|jsx)'",
      "prepare-commit-msg": ""
    }
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "async": "^3.1.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^10.0.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-0": "^6.24.1",
    "chalk": "^2.4.2",
    "commander": "^2.20.0",
    "download-git-repo": "^2.0.0",
    "eslint": "^5.6.0",
    "handlebars": "^4.1.2",
    "inquirer": "^6.4.1",
    "metalsmith": "^2.3.0",
    "ora": "^3.4.0",
    "rimraf": "^2.6.3"
  },
  "prettier": {
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "disableLanguages": [
      "vue"
    ],
    "eslintIntegration": false,
    "ignorePath": ".prettierignore",
    "jsxBracketSameLine": false,
    "parser": "babel",
    "printWidth": 80,
    "proseWrap": "preserve",
    "requireConfig": false,
    "semi": false,
    "singleQuote": false,
    "stylelintIntegration": false,
    "tabWidth": 4,
    "trailingComma": "none",
    "useTabs": false
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@types/metalsmith": "^2.3.0",
    "husky": "^3.0.8"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
